<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="PrimaryAccentSolidColorBrush" Color="{DynamicResource PrimaryAccentColor}" />
    <SolidColorBrush x:Key="ErrorSolidColorBrush" Color="{DynamicResource ErrorColor}" />

    <SolidColorBrush x:Key="PrimaryBackgroundSolidColorBrush" Color="{DynamicResource PrimaryBackgroundColor}" />
    <SolidColorBrush x:Key="SecondaryBackgroundSolidColorBrush" Color="{DynamicResource SecondaryBackgroundColor}" />
    <SolidColorBrush x:Key="PanelBackgroundSolidColorBrush" Color="{DynamicResource PanelBackgroundColor}" />
    <SolidColorBrush x:Key="WindowBlurBackgroundSolidColorBrush" Color="{DynamicResource WindowBlurBackgroundColor}" />

    <SolidColorBrush x:Key="SeparatorSolidColorBrush" Color="{DynamicResource SeparatorColor}" />
    <SolidColorBrush x:Key="WindowBlurSeparatorSolidColorBrush" Color="{DynamicResource WindowBlurSeparatorColor}" />

    <SolidColorBrush x:Key="DisabledTextSolidColorBrush" Color="{DynamicResource DisabledTextColor}" />
    <SolidColorBrush x:Key="NormalTextSolidColorBrush" Color="{DynamicResource NormalTextColor}" />
    <SolidColorBrush x:Key="SelectedTextSolidColorBrush" Color="{DynamicResource SelectedTextColor}" />

</ResourceDictionary>
