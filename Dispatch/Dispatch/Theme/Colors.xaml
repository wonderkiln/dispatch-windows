<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="PrimaryAccentColor" Color="#017AFF" />
    <SolidColorBrush x:Key="ErrorColor" Color="#FF0138" />

    <SolidColorBrush x:Key="PrimaryBackgroundColor" Color="#FFFFFF" />
    <SolidColorBrush x:Key="SecondaryBackgroundColor" Color="#FAFAFA" />

    <SolidColorBrush x:Key="SeparatorColor" Color="#DFDFDF" />

    <SolidColorBrush x:Key="DisabledTextColor" Color="#99000000" />
    <SolidColorBrush x:Key="NormalTextColor" Color="#000000" />
    <SolidColorBrush x:Key="SelectedTextColor" Color="#FFFFFF" />

</ResourceDictionary>
