<Window x:Class="Dispatch.Screen.QueueWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dispatch.Screen"
        mc:Ignorable="d"
        Title="QueueWindow" Height="450" Width="800">
    <ListBox ItemsSource="{Binding QueueViewModel.List, RelativeSource={RelativeSource AncestorType={x:Type local:QueueWindow}}}">
        <ListBox.ItemTemplate>
            <DataTemplate>
                <TextBlock Text="Something" />
            </DataTemplate>
        </ListBox.ItemTemplate>
    </ListBox>
</Window>
